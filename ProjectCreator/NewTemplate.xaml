<Window x:Class="ProjectCreator.NewTemplate"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectCreator"
        mc:Ignorable="d"
        Title="NewTemplate" Height="136.098" Width="300">
    <Grid>
        <Label x:Name="templateNameLabel" Content="Enter Template Name" Height="25" Width="125" VerticalAlignment="Top"></Label>
        <TextBox x:Name="templateName" Height="25" Width="250" VerticalAlignment="Top" Margin="0,25,0,0"></TextBox>
        <Button x:Name="OkButton" Content="Confirm" Height="25" Width="50" Margin="75,65,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Click="OkButton_OnClick"></Button>
        <Button x:Name="CancelButton" Content="Cancel" Height="25" Width="50" Margin="150,65,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Click="CancelButton_OnClick"></Button>
    </Grid>
</Window>
